X -> NTS TS RS AX

NTS -> non-terminal IL ;
IL -> id ILt
ILt -> , id ILt
ILt -> ''

TS -> terminal ISL ;
ISL -> E ISLt
ISLt -> , E ISLt
ISLt -> ''

RS -> R ; RSt ;
RSt -> R ; RSt
RSt -> ''

R -> id ::= SS
SS -> S SSt
SSt -> | S SSt 
SSt -> ''

S -> epsilon
S -> E St
St -> E St 
St -> ''

E -> id
E -> str

AX -> axiom id ;
